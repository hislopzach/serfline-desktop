{
  "name": "linehacker",
  "version": "0.1.0",
  "private": true,
  "author": "Hach Zislop",
  "homepage": "./",
  "main": "public/electron.js",
  "devDependencies": {
    "electron-builder": "^22.11.7",
    "electron": "^13.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hislopzach/linehacker"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.21.1",
    "concurrently": "^6.2.1",
    "date-fns": "^2.14.0",
    "electron-is-dev": "^2.0.0",
    "firebase": "^7.14.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-player": "^2.9.0",
    "react-query": "^1.5.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "recharts": "^1.8.5",
    "sass": "^1.32.12",
    "source-map-explorer": "^2.5.0",
    "wait-on": "^6.0.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "deploy": "npm run build && firebase deploy",
    "eject": "react-scripts eject",
    "el-build": "electron-builder build --mac --win --publish never",
    "el-deploy": "electron-builder build --mac --win --publish always",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-package": "./node_modules/.bin/electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-package": "npm run build"
  },
  "build": {
    "appId": "linehacker",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
